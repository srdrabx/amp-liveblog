  <body>
    <div class="header">
        <img src="https://www.spacex.com/sites/spacex/files/spacex_logo_white.png" alt="">
    </div>
    <h1 class="countdown"><amp-date-countdown timestamp-seconds="1587676560"
      layout="fixed-height"
      height="100"
      when-ended="continue"
      biggest-unit="hours">
      <template type="amp-mustache">
        T-{{hh}}:{{mm}}:{{ss}}
      </template>
    </amp-date-countdown></h1>

    <amp-live-list 
        id="amp-live-list-videos"
        data-poll-interval="30"
        data-max-items-per-page="<%= pageSize %>"
        <% if (currentPage > 1) { %>  disabled  <% } %>
    >

    <button id ="live-list-update-button" update on="tap:amp-live-list-videos.update" >You have updates</button>

    <div items id="timeline-content">
      <ul class="timeline">

      <% posts.forEach((post) => { %>
      <div class="post" id="<%= post.id %>" data-sort-time="<%= post.timestamp %>">
        <li class="event" data-date="T<%= post.time %>">
          <h3><%= post.title %></h3>
          <p><%= post.text %></p>
          <amp-img src="<%= post.image %>"
            width="1080"
            height="610"
            layout="responsive"
            alt="AMP"></amp-img>
        </li>
      </div>
      <% }) %>
    </ul>
    </div>

    <div pagination>
      <nav aria-label="amp live list pagination">

        <% if (pageCount > 1) { %>
        <ul class="pagination">
          
          <% for (var i = 1; i <= pageCount; i++) { %>
            <% if (currentPage == i) { %>
              <li><%= i %></li>
            <% } else { %>
              <li><a href="/?page=<%= i %>"><%= i %></a></li>
            <% } %>
          <% } %>
        
        </ul>
        <% } %>  
      </nav>
    </div>

  </amp-live-list>
  
